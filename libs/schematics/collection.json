{
  "$schema": "./node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Adds MetaUI ngx-meta/rules to the application ",
      "factory": "./ng-add/index",
      "schema": "./ng-add/add-schema.json"
    },
    "init-project": {
      "description": "Configure project workspace with MetaUI ",
      "private": true,
      "factory": "./ng-add/init-project/index",
      "schema": "./ng-add/add-schema.json"
    },
    "mPage": {
      "description": "Generate simple MetaUI based page with modelClass and rules",
      "factory": "./ng-generate/m-page/index",
      "schema": "./ng-generate/m-page/page-schema.json"
    }
  }
}
